{
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "require": [
    "babel-core/register",
    "./tests/helpers/test-setup.js"
  ],
  "include": [
    "src/**/*.js",
    "tests/helpers/mount.js"
  ],
  "exclude": [],
